{
    "name": "radic",
    "version": "1.0.0",
    "description": "mono",
    "private": true,
    "workspaces": {
        "packages": [
            "apps/cli",
            "packages/console",
            "packages/console-colors",
            "packages/console-input",
            "packages/console-output",
            "packages/core",
            "packages/shared"
        ],
        "nohoist": []
    },
    "resolutions2": {
        "before-after-hook": "1.4."
    },
    "scripts": {
        "docs:dev": "vuepress dev src",
        "docs:build": "vuepress build src",
        "docs:info": "vuepress info src",
        "core": "cd packages/core && yarn",
        "shared": "cd packages/shared && yarn",
        "console": "cd packages/console && yarn",
        "console-colors": "cd packages/console-colors && yarn",
        "console-input": "cd packages/console-input && yarn",
        "console-output": "cd packages/console-output && yarn",
        "app:cli": "cd apps/cli && yarn",
        "watch": "yarn build && concurrently \"yarn watch:tools\" \"yarn watch:util\"",
        "reinstall": "yarn clean:yarn && yarn",
        "build": "npm-run-all \"shared build\" \"core build\" \"console-output build\" \"console-input build\" \"console-colors build\" \"console build\"",
        "test": "npm-run-all \"shared test\" \"core test\" \"console-output test\" \"console-input test\" \"console-colors test\" \"console test\"",
        "clean:build": "rimraf **/lib **/.nyc_output",
        "clean:test": "rimraf **/.nyc_output",
        "clean:yarn": "rimraf \"**/node_modules\" \"**/yarn.lock\" \"**/.nyc_ou*\""
    },
    "dependencies": {
        "@babel/core": "^7.16.0",
        "@types/chai": "^4.2.22",
        "@types/cross-spawn": "^6.0.2",
        "@types/debug": "^4.1.7",
        "@types/del": "^4.0.0",
        "@types/dotenv": "^6.1.1",
        "@types/fs-extra": "^9.0.13",
        "@types/lodash": "^4.14.177",
        "@types/lz-string": "^1.3.34",
        "@types/mocha": "^9.0.0",
        "@types/node": "^16.11.10",
        "@types/sinon": "^10.0.6",
        "@types/sinon-chai": "^3.2.5",
        "@types/superagent": "^4.1.13",
        "@vuepress/plugin-active-header-links": "^2.0.0-beta.27",
        "@vuepress/plugin-back-to-top": "^1.8.2",
        "@vuepress/plugin-medium-zoom": "^1.8.2",
        "@vuepress/plugin-nprogress": "^1.8.2",
        "@vuepress/plugin-register-components": "^1.8.2",
        "@vuepress/plugin-search": "^1.8.2",
        "chai": "^4.3.4",
        "chai-sinon": "^2.8.1",
        "concurrently": "^6.4.0",
        "cross-env": "^7.0.3",
        "debug": "^4.3.2",
        "del": "^6.0.0",
        "fs-extra": "^10.0.0",
        "lodash": "^4.17",
        "mocha": "^6.2.3",
        "mocha-typescript": "^1.1.17",
        "np": "^7.6.0",
        "npm-run-all": "^4.1.5",
        "nps": "^5.10.0",
        "nps-utils": "^1.7.0",
        "nyc": "^14.1.1",
        "release-it": "^14.11.8",
        "renamer": "^4.0.0",
        "rimraf": "^3.0.2",
        "sinon": "^7.5.0",
        "sinon-chai": "^3.6.0",
        "source-map-support": "^0.5.21",
        "superagent": "^6.1.0",
        "tap-nyc": "^1.0.3",
        "ts-mocha": "^7.0.0",
        "ts-node": "^10.4.0",
        "tslib": "^2.3.1",
        "typescript": "^4.6.0-dev.20211126",
        "yarn-run-each": "^1.2.0"
    },
    "devDependencies": {
        "@vue/component-compiler-utils": "^3.3.0",
        "@vuepress/plugin-prismjs": "^2.0.0-beta.27",
        "@vuepress/plugin-toc": "^2.0.0-beta.27",
        "vue": "^3.2.23",
        "vuepress": "^2.0.0-beta.27",
        "vuepress-plugin-typedoc": "^0.9.2",
        "typedoc": "^0.22.10",
        "typedoc-plugin-markdown": "^3.11.7",
        "typedoc-plugin-extras": "^2.2.1",
        "typedoc-plugin-inline-sources": "^1.0.1",
        "typedoc-plugin-merge-modules": "^3.0.2",
        "typedoc-plugin-mermaid": "^1.6.0"
    }
}
