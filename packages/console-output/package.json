{
    "name": "@radic/console-output",
    "version": "1.5.4",
    "description": "Console output",
    "author": {
        "name": "Robin Radic"
    },
    "publishConfig": {
        "access": "public"
    },
    "license": "MIT",
    "main": "lib/index.js",
    "types": "lib/index.d.ts",
    "typings": "lib/index.d.ts",
    "scripts": {
        "test": "nyc mocha --opts test/mocha.opts",
        "build": "tsc --project tsconfig.build.json",
        "watch": "tsc --project tsconfig.build.json --watch",
        "clean:build": "rm -rf lib",
        "clean:yarn": "rm yarn.lock node_modules/",
        "prepublish": "yarn run clean:build && yarn run build",
        "release": "yarn exec np --yolo --no-yarn",
        "fixtrucolor": "node ./fixTrucolor.js",
        "postinstall": "node ./fixTrucolor.js"
    },
    "dependencies": {
        "cli-truncate": "^2.1.0",
        "color-convert": "^2.0.1",
        "columnify": "^1.5.4",
        "d": "^1.0.1",
        "indent-string": "^4.0.0",
        "lodash": "^4.17.21",
        "memory-streams": "^0.1.3",

        "slice-ansi": "^4.0.0",
        "string-width": "^4.2.2",
        "strip-ansi": "^6.0.0",
        "supports-color": "^8.1.1",
        "term-size": "^2.2.1",
        "terminal-kit": "^1.49.3",
        "trucolor": "^2.0.4",
        "widest-line": "^3.1.0",
        "wrap-ansi": "^7.0.0",
        "yarn-run-all": "^3.1.1"
    },
    "optionalDependencies": {
        "node-notifier": "^10.0.0"
    },
    "peerDependencies": {
        "listr": "^0.14.3",
        "progress": "^2.0.3",
        "sparkly": "^5.0.0",
        "multispinner": "^0.2.1",
        "ora": "^5.4.0",
        "archy": "^1.0.0",
        "beeper": "^2.1.0",
        "chalk": "^4.1.0",
        "cli-highlight": "^2.1.11",
        "cli-table2": "^0.2.0",
        "cli-table3": "^0.6.0"
    },
    "devDependencies": {
        "listr": "^0.14.3",
        "progress": "^2.0.3",
        "sparkly": "^5.0.0",
        "multispinner": "^0.2.1",
        "ora": "^5.4.0",
        "archy": "^1.0.0",
        "beeper": "^2.1.0",
        "chalk": "^4.1.0",
        "cli-highlight": "^2.1.11",
        "cli-table2": "^0.2.0",
        "cli-table3": "^0.6.0",
        "@types/ansi": "^0.3.2",
        "@types/archy": "^0.0.32",
        "@types/cli-table2": "^0.2.3",
        "@types/cli-truncate": "^1.1.0",
        "@types/color-convert": "^2.0.0",
        "@types/d": "^1.0.1",
        "@types/indent-string": "^3.2.0",
        "@types/listr": "^0.14.4",
        "@types/node-notifier": "^8.0.1",
        "@types/progress": "^2.0.5",
        "@types/slice-ansi": "^5.0.0",
        "@types/strip-ansi": "^3.0.0",
        "@types/supports-color": "^8.1.1",
        "@types/wrap-ansi": "^8.0.1"
    },
    "engines": {
        "node": ">=8.0.0"
    },
    "files": [
        "/bin",
        "/lib",
        "/npm-shrinkwrap.json",
        "/oclif.manifest.json"
    ],
    "oclif": {
        "commands": "./lib/cli/commands",
        "bin": "sites",
        "plugins": [
            "@oclif/plugin-help"
        ]
    }
}
