{
    "name": "@radic/console",
    "version": "1.0.1",
    "description": "Create class based commands with Yargs using decorators",
    "keywords": [
        "yargs",
        "class",
        "decorators",
        "cli",
        "commands"
    ],
    "author": {
        "name": "Robin Radic"
    },
    "publishConfig": {
        "access": "public"
    },
    "license": "MIT",
    "main": "lib/index.js",
    "types": "lib/index.d.ts",
    "typings": "lib/index.d.ts",
    "scripts": {
        "start": "npm-run-all clean:build build",
        "test": "nyc mocha --opts test/mocha.opts",
        "build": "yarn clean:build && tsc --project tsconfig.build.json",
        "watch": "tsc --project tsconfig.build.json --watch",
        "clean:build": "rm -rf lib && rimraf src/**/*.{js,d.ts}",
        "clean:yarn": "rm yarn.lock node_modules/",
        "prepare": "npm-run-all clean:build build",
        "prepublish": "npm-run-all clean:build build",
        "login": "yarn login",
        "release": "npm-run-all clean:build build login && np --yolo --no-yarn"
    },
    "dependencies": {
        "@radic/core": "^1.0.0",
        "@radic/shared": "^1.0.0",
        "@types/dot-object": "^2.1.2",
        "@types/lz-string": "^1.3.34",
        "collect.js": "^4.29.0",
        "dot-object": "^2.1.4",
        "dotenv": "^10.0.0",
        "env-paths": "^3.0.0",
        "eventemitter2": "^6.4.5",
        "findup-sync": "^5.0.0",
        "@types/findup-sync": "^4.0.1",
        "glob": "^7.2.0",
        "hasbin": "^1.2.3",
        "ini": "^2.0.0",
        "inversify": "^6.0.1",
        "inversify-binding-decorators": "^4.0.0",
        "inversify-inject-decorators": "^3.1.0",
        "ip": "^1.1.5",
        "lodash": "^4.17.21",
        "lowdb": "^3.0.0",
        "lz-string": "^1.4.4",
        "module-alias": "^2.2.2",
        "nginx-conf": "^2.0.0",
        "open-editor": "^4.0.0",
        "reflect-metadata": "^0.1.13",
        "semver": "^7.3.5",
        "simple-hosts": "^1.2.3",
        "sqlite3": "^5.0.2",
        "systeminformation": "^5.9.9",
        "tapable": "^2.2.1",
        "tonto": "^0.1.1",
        "typeorm": "^0.2.38",
        "which": "^2.0.2",
        "winston": "^3.3.3",
        "yargs": "file:./yargs"
    },
    "devDependencies": {
        "@types/chalk": "^2.2.0",
        "@types/eventemitter2": "^4.1.0",
        "@types/hasbin": "^1.2.0",
        "@types/ini": "^1.3.31",
        "@types/ip": "^1.1.0",
        "@types/jquery": "^3.5.8",
        "@types/jsdom": "^16.2.13",
        "@types/lodash": "^4.14.176",
        "@types/node": "^16.11.6",
        "@types/semver": "^7.3.9",
        "@types/systeminformation": "^3.54.1",
        "@types/which": "^2.0.1",
        "@types/yargs": "^17.0.5",
        "@types/yargs-parser": "^20.2.1"
    },
    "private": false,
    "publishConfig": {
        "access": "public",
        "description": true
    },
    "engines": {
        "node": ">=8.0.0"
    }
}
