{
    "name": "@radic/hosting",
    "private": false,
    "version": "1.0.1",
    "description": "Manage and update multiple package.json files. Useful for monorepos among other things",
    "author": {
        "name": "Robin Radic",
        "email": "rradic@hotmail.com",
        "url": "https://github.com/robinradic"
    },
    "license": "MIT",
    "main": "lib/index.js",
    "type": "commonjs",
    "types": "lib/index.d.ts",
    "typings": "lib/index.d.ts",
    "homepage": "https://robinradic.github.io/npm-packages/packages/multi-package-json-manager",
    "scripts": {
        "build": "yarn clean:build && tsc --project tsconfig.build.json",
        "clean:build": "rm -rf lib && rimraf src/**/*.{js,js.map,d.ts}",
        "clean:docs:api": "rm -rf docs/api",
        "clean:test": "rm -rf .nyc-output/",
        "clean:yarn": "rm yarn.lock node_modules/",
        "docs:api": "rm -rf docs/api && node ../../typedoc.js",
        "prepublish": "npm-run-all clean:build test build",
        "release": "yarn login && np --yolo --no-yarn",
        "start": "npm-run-all clean:build test build",
        "pretest": "yarn clean:test",
        "test": "mocha --opts test/mocha.opts",
        "posttest": "yarn clean:test",
        "watch": "tsc --project tsconfig.build.json --watch"
    },
    "dependencies": {
        "@types/chalk": "^2.2.0",
        "@types/eventemitter2": "^4.1.0",
        "@types/hasbin": "^1.2.0",
        "@types/ini": "^1.3.31",
        "@types/ip": "^1.1.0",
        "@types/jquery": "^3.5.8",
        "@types/jsdom": "^16.2.13",
        "@types/lodash": "^4.14.176",
        "@types/semver": "^7.3.9",
        "@types/which": "^2.0.1",
        "@radic/shared": "^1.0.1",
        "lodash": "^4.17.21",
        "axios": "^0.24.0",
        "@types/dot-object": "^2.1.2",
        "@types/lz-string": "^1.3.34",
        "apacheconf": "^0.0.5",
        "dot-object": "^2.1.4",
        "dotenv": "^10.0.0",
        "env-paths": "^3.0.0",
        "eventemitter2": "^6.4.5",
        "findup-sync": "^5.0.0",
        "@types/findup-sync": "^4.0.1",
        "glob": "^7.2.0",
        "hasbin": "^1.2.3",
        "ini": "^2.0.0",
        "ip": "^1.1.5",
        "lz-string": "^1.4.4",
        "module-alias": "^2.2.2",
        "nginx-conf": "^2.0.0",
        "open-editor": "^4.0.0",
        "reflect-metadata": "^0.1.13",
        "semver": "^7.3.5",
        "simple-hosts": "^1.2.3",
        "tonto": "^0.1.1",
        "jquery": "^3.6.0",
        "jsdom": "^18.0.1",
        "which": "^2.0.2",
        "typeorm": "^0.2.41"
    },
    "devDependencies": {
        "@radic/core": "^1.0.0"
    },
    "peerDependencies": {
        "@radic/core": "^1.0.0"
    },
    "keywords": [
        "format",
        "formatter",
        "json",
        "package",
        "package.json"
    ],
    "bugs": {
        "email": "rradic@hotmail.com",
        "url": "https://github.com/robinradic/npm-packages/issues"
    },
    "engines": {
        "node": ">=12"
    },
    "os": [
        "darwin",
        "linux"
    ],
    "publishConfig": {
        "access": "public"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/robinradic.git",
        "directory": "packages/multi-package-json-manager"
    },
    "vuepress": {
        "name": "Hosting Library",
        "destDir": "hosting"
    }
}
